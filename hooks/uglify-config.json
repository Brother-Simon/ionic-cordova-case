{
    "alwaysRun": false,
    "recursiveFolderSearch": true,
    "foldersToProcess": [
        "js",
        "css"
    ],
    "uglifyJsOptions": {
        "compress": {
        "sequences": true,
        "dead_code": true,
        "conditionals": true,
        "booleans": true,
        "unused": true,
        "if_return": true,
        "join_vars": true,
        "drop_console": true
        },
        "fromString": true
    },
    "cleanCssOptions": {
        "noAdvanced": true,
        "keepSpecialComments": 0
    }
}